      "source": [
        "def gradient(m1,c,X,Y):\n",
        "  m1_deriv=-1*(Y-m1*X)*X\n",
        "  c_deriv=-1*(Y-m1*X-c)\n",
        "  return m1_deriv,c_deriv"
      ],
      "execution_count": 66,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"

      },
      "source": [
        "for iter in range(max_iter):\n",
        "  for sample in range(x_train.shape[0]):\n",
        "    del_m1,del_c=gradient(m1, c, x_train[sample,0], y_train[sample])\n",
        "    m1=m1-eta*del_m1\n",
        "    c=c-eta*del_c\n",
        "print('model parameter m and c',m1,c)"

        "id": "sXTAiKP70Nol"
      },
      "source": [
        "outliers = []\n",
        "def detect_outliers_zscore(data):\n",
        "     thres = 2\n",
        "     mean = np.mean(data)\n",
        "     std = np.std(data)\n",
        "     for i in data:\n",
        "       z_score = (i-mean)/std\n",
        "       if (np.abs(z_score))>thres:\n",
        "        outliers.append(i)\n",
        "       return outliers"
      ],
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
